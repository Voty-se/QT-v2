@model IEnumerable<QT.Models.StandarProduct>

<table class="table">
    <tr>
        <th>
            Produkt
        </th>
        <th>
            Pris
        </th>
        <th>
            Monteringstid i snitt
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)  @Html.DisplayFor(modelItem => item.Text)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Time)
        </td>
        <td>
            <a onclick="removeProduct(@item.Id);">Tabort</a>
        </td>
    </tr>
}

</table>
